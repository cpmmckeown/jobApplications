$(function() {
  MailChecker = function(input) {
    this._input = input;
  };

  $.extend(MailChecker.prototype, {
    validate: function() {
      var mailChecker = this;
      mailChecker._input.mailcheck({
        suggested: function (_element, suggestion) {
          mailChecker._removeWarning();
          mailChecker._addSuggestion(suggestion['full']);
        },
        empty: function (element) {
          mailChecker._removeWarning();
        }
      });
    },

    _addSuggestion: function(suggestion) {
      var suggestionLink = $("<a href='#' class='suggestion-link' />").text(suggestion);
      var markup = $("<div class='mailcheck-suggestion text-warning'></div>")
        .append(Components.iconic('warning'))
        .append(" Do you mean ").append(suggestionLink).append("?");

      this._input.after(markup);
      this._addSuggestionLinkListener(suggestionLink);
    },

    _removeWarning: function() {
      this._input.parent().find('div.mailcheck-suggestion').remove();
    },

    _addSuggestionLinkListener: function(link) {
      var mailChecker = this;
      
      link.click(function (e) {
        e.preventDefault();
        mailChecker._input.val($(this).text());
        mailChecker._removeWarning();
      });
    }
  });

  $(document).on('blur', 'input[type=email]', function() {
    if($(this).data('email-suggestion') === false) return;
    new MailChecker($(this)).validate();
  });
});
// this file powers the modal editors used for inline editing

$(function() {
  window.ModalEditor = {
    open: function(selector, opts) {
      var modal = $(selector);
      if (modal.length != 1) return false;
    
      if (opts.error_message || opts.error_fields) {
        ModalEditor.fillErrors(modal, opts);
      }
      ModalEditor.fill(modal, opts.field_values);
      modal.modal('show');
      return true;
    },
    
    find: function(modal) {
      return $(modal).is('.modal') ? $(modal) : $(modal).find('.modal');
    },
    
    hideAndThen: function(modal, then) {
      ModalEditor.find(modal).
        on('hidden.bs.modal', then || $.noop).
        modal('hide');
    },
    
    ajax: function(form, settings) {
      return $.ajax(settings)
      .done(function(json, status, xhr) {
        ModalEditor.analytics(form, 'editor-success');
        ModalEditor.ajaxSuccess(form, json, status, xhr);
      })
      .fail(function(xhr, errorname, error) {
        ModalEditor.analytics(form, 'editor-failure');
        ModalEditor.ajaxError(form, xhr, errorname, error);
      })
    },
    
    ajaxSuccess: function(modal, json, status, xhr) {
      var modal = $(modal);
    
      if (xhr && xhr.getResponseHeader('Content-Type').match(/text\/html/)) {
        json = {section: json};
      }
    
      if (json.section || json.updates) {
        ModalEditor.hideAndThen(modal, function() {
          modal.trigger('before-success.modal-editor');
          ModalEditor.applyUpdates(json, modal);
          ModalEditor.resetSubmit(modal);
          $("#" + modal.attr('id')).trigger('success.modal-editor');
        });
      } else {
        ModalEditor.fillErrors(modal, json);
        modal.trigger('error.modal-editor');
      }
    },
    
    ajaxError: function(modal, xhr, errorname, error) {
      var json = xhr.responseJSON;
      if (json && json['updates']) {
        FindAndUpdate.htmlImmediate(json.updates);
        ModalEditor.resetSubmit(modal);
      } else {
        if (error) json = json || {error_message: error.message, error_object: error, xhr: xhr};
        ModalEditor.fillErrors(modal, json);
      }
    },
    
    applyUpdates: function(json, eventTrigger) {
      if (json.section && eventTrigger) {
        json.updates = {};
        json.updates[eventTrigger.attr('data-replaces')] = json.section;
        delete json.section;
      }
      
      FindAndUpdate.html(json.updates).then(function() {
        FindAndUpdate.classes(json.classes);
        FindAndUpdate.text(json.text_updates);
        FindAndUpdate.glow(json.glows !== undefined ? json.glows : {updated: eventTrigger.attr('data-replaces')});
      });
    },
    
    fill: function(modal, values) {
      $.each(values || {}, function(field, value) {
        $(modal).find(".form-group." + field + " :input").val(value).select2("val",value);
      });
    
    },
    
    fillErrors: function(modal, errors) {
      modal = $(modal);
      errors = errors || {};
      errorsAlert = modal.find('.alert-danger.could-not-save');
      modal.data('last-errors', errors);
      
      ModalEditor.clearErrors(modal);
      
      errorsAlert.show().find('.title').text(errors.error_title);
      errorsAlert.show().find('.text').text(errors.error_message || "An error occurred.");
      
      var leftovers = [];
      $.each(errors.field_errors || {}, function(field, messages) {
        var group = modal.find('.form-group.' + field);
        var controls = group.find('.controls');
        var controlsHint = controls.find('.help-block');
        group.addClass('has-error');
        $.each(messages, function() {
          var helpBlock = $('<span class="help-block error">').text(this);
          controlsHint.length == 0 ? controls.append(helpBlock) : controlsHint.prepend(helpBlock);
          if (group.length == 0) {leftovers.push({field: field, message: this})}
        });
      });
      
      errorsAlert.find('ul.leftover-errors').remove();
      if (leftovers.length > 0) {
        var leftoversList = $('<ul class="leftover-errors starts-hidden">');
        $.each(leftovers, function() {
          leftoversList.append($('<li>').text(this['field'] + " " + this['message']));
          ModalEditor.analytics(modal, "leftover-" + this['field']);  
        });
        errorsAlert.append(leftoversList);
      }
      
      ModalEditor.resetSubmit(modal);
    },
    
    clearErrors: function(modal) {
      modal = $(modal);
      modal.find('.alert-danger').hide();
      modal.find('.form-group').removeClass('has-error');
      modal.find('.help-block.error').remove();
    },
    
    resetSubmit: function(modal) {
      $(modal).find('.once').trigger('reset.once');
    },
    
    analytics: function(modal, event) {
      modal = ModalEditor.find(modal);
      analytics.event('modals', event, modal.attr('data-analytics-id') || modal.attr('id'));
    }
  };
  
  $(document).on('shown.bs.modal', 'form.modal-editor .modal', function(event) {
    if ($(this).is('.no-autofocus')) return;
    $(this).find('.modal-body :input').first().focus(function () {
      if (this.tagName != 'textarea') return;

      this.selectionStart = this.value.length;
      this.selectionEnd = this.value.length;
    }).focus();
  });
  $(document).on('ajax:aborted:file', 'form.modal-editor', function(event, elements) {
    ModalEditor.analytics($(this).find('.modal'), 'file-fallback');
    
    var form = $(elements[0].form);
    form.attr('action', form.attr('action').replace('.json', ''));
    if (form.find('input[name=authenticity_token]').length == 0) {
      var token = $('meta[name="csrf-token"]').attr('content');
      var tokenInput = $('<input type="hidden" name="authenticity_token"/>').val(token);
      form.find('input[name=utf8]').after(tokenInput);
    }
  });
  $(document).on('ajax:success', 'form.modal-editor', function(event, json, status, xhr) {
    ModalEditor.analytics(this, 'editor-success');
    ModalEditor.ajaxSuccess($(this), json, status, xhr);
  });
  $(document).on('ajax:error', 'form.modal-editor', function(event, xhr, errorname, error) {
    ModalEditor.analytics(this, 'editor-failure');
    ModalEditor.ajaxError($(this), xhr, errorname, error);
  });

  $(document).on('ajax:success', 'a.delete[data-replaces]', function(event, json) {
    if (json.error_message) {
      ModalEditor.analytics(this, 'delete-failure');
      alert(json.error_message || "An error occurred.");
    } else {
      var handleSuccess = function() {
        ModalEditor.analytics(this, 'delete-success');
        ModalEditor.applyUpdates(json, $(event.currentTarget));
      };
      
      var editor = ModalEditor.find($(this).closest('.modal'));
      editor.length ? ModalEditor.hideAndThen(editor, handleSuccess) : handleSuccess();
    }
  });
  
  ModalEditor.open((document.location.hash || 'match-nothing') + ".modal", $.url().param()) ||
  ModalEditor.open($('#modal-auto-open').attr('data-selector'), JSON.parse($('#modal-auto-open').text() || null));
});

$(function() {
  window.Once = {
    disable: function($this) {
      var replacedBy = $this.clone().addClass('once-placeholder').prop('disabled', true);
      $this.data('replaced-by', replacedBy);
    
      var newText = $this.attr('data-active-text');
      ({
