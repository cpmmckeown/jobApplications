  tyrantEls.on('click', '.column-selector .column', function(event) {
    if (!$(event.target).is('.column, .column span')) return;
    TableTyrant.Grid.closest(event.target).columns().toggleVisible($(event.target).closest('.column'));
  });
  tyrantEls.each(function(tyrantEl) {
    TableTyrant.Grid.closest(tyrantEl).columns();
  });
  tyrantEls.find('.column-selector ol').sortable({
    cursor: 'move',
    handle: '.grip',
    opacity: .75,
    placeholder: 'sorting-placeholder',
    connectWith: '.column-selector ol',
    start: function(event, ui) {
      TableTyrant.Grid.closest(event.target).columns().showDragOptions($(this));
    },
    over: function(event, ui) {
      var grid = TableTyrant.Grid.closest(event.target);
      var isHidden = grid.columns().isHiddenList(this);
      var wasHidden = grid.columns().isHiddenList(ui.sender);
      
      ui.placeholder.toggleClass('hidden', isHidden && wasHidden);
      
      if (isHidden) {
        ui.placeholder.text('hide column');
      } else if (wasHidden) {
        ui.placeholder.text('show column');
      } else {
        ui.placeholder.text('reorder column');
      }
    },
    stop: function(event, ui) {
      var grid = TableTyrant.Grid.closest(event.target);
      grid.columns().hideDragOptions(ui);
      grid.columns().sortHiddenList();
    },
    remove: function(event, ui) {
      var grid = TableTyrant.Grid.closest(event.target);
      var wasHidden = grid.columns().isHiddenList(this);
      grid.event('table-tyrant.column-' + (wasHidden ? 'checked' : 'unchecked'), '-' + ui.item.data('column-name'));
    },
    update: function(event, ui) {
      var grid = TableTyrant.Grid.closest(event.target);
      if (grid.columns().isHiddenList(this)) {return}
      
      TableTyrant.Grid.closest(event.target).columns().updateOrder();
    }
  });
  
  // sorting
  tyrantEls.on('click', 'thead tr.section-headers th:not(.unsortable)', function(e) {
    var th = $(e.target);
    if (th.is(':input, a')) return;
    
    e.preventDefault();
    
    var $gridEl = th.closest('.table-tyrant');
    var grid = TableTyrant.Grid.closest($gridEl);
    var asc = th.is('.sort-descending');
    grid.event(asc ? 'sort-ascending' : 'sort-descending', '-' + th.attr('data-sort'));
    $gridEl.find('input[type=hidden][name=page]').val(1);
    $gridEl.find('input[type=hidden][name=sort]').val(th.attr('data-sort'));
    $gridEl.find('input[type=hidden][name=sort-dir]').val(asc ? 'asc' : '');
    $gridEl.find('.edit-workspace').submit();
  });
  
  // selection checkboxes
  tyrantEls.on('click', 'thead tr.section-headers th.selected input[name=select-all]', function(e) {
    $(this).closest('.table-tyrant').find('tr td.selected input').prop('checked', e.target.checked);
  });
  tyrantEls.on('click', 'tr td.selected input', function(e) {
    if (e.target.checked) return;
    
    $(this).closest('.table-tyrant').find('thead tr.section-headers th.selected input[name=select-all]').prop('checked', false);
  });
  
  // paging
  tyrantEls.on('click', '.more-pages-to-see *[data-page]', function(e) {
    e.preventDefault();
    
    var page = $(e.target).attr('data-page');
    var $gridEl = $(this).closest('.table-tyrant');
    var grid = TableTyrant.Grid.closest($gridEl);
    
    grid.event('page', '-' + page);
    
    $gridEl.find('input[type=hidden][name=page]').val(page);
    $gridEl.find('.edit-workspace').submit();
  });

  tyrantEls.on('ajax:send', 'form.edit-workspace, form.filter-sidebar', function(event) {
    $(event.target).closest('.table-tyrant').addClass('refreshing');
    $(this).find('.modal').modal('hide');
  });

  tyrantEls.on('ajax:complete', 'form.edit-workspace, form.filter-sidebar', function(event) {
    var $form = $(event.target);
    $form.closest('.table-tyrant').removeClass('refreshing');
  });

  tyrantEls.on('ajax:success', 'form.edit-workspace, form.filter-sidebar', function(event) {
    TableTyrant.Grid.closest($(event.target)).updateUrlFromForm();
  });

  tyrantEls.on('ajax:error', 'form.edit-workspace, form.filter-sidebar', function(event) {
    Once.reset($(event.target).find('.once-hidden'));
  });
}
  
$(window).on("popstate", function(e) {
  var state = e && e.originalEvent && e.originalEvent.state;
  if (!state || !state.tableTyrantId) return;
  
  TableTyrant.Grid.updateGridFromState(state);
});

