        button: function () { replacedBy.html($this.attr('data-active-text') || $this.html())}
      }[$this[0].tagName.toLowerCase()])();
    
      $this.after(replacedBy);
      $this.toggleClass('once-hidden', true);
    
      $this.add(replacedBy).on('reset.once', function() {
        Once.reset($this);
      });
