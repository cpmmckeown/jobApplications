      fn(this);
      root.append(this.form);
      return this;
    },
    input: function(name, options) {
      return $('<div>')
        .appendTo(this.form)
        .addClass('form-group')
        .addClass(name)
        .addClass(this._formOptions.as ? this._formOptions.as + "_" + name : '')
        .toggleClass('hidden', options.hidden === true)
        .append(this.label(name, options))
        .append($('<div>')
          .addClass('controls')
          .append(this.inputElement(name, options))
          .append(options.hint)
        );
    },
    label: function(name, options) {
      return $('<label>')
        .addClass('control-label')
        .append(options.label || this.humanizedString(name))
        .toggleClass('required', options.required);
    },
    inputElement: function(name, options) {
      return (options.input || $('<' + (options.as || 'input') + '>'))
        .addClass('form-control')
        .toggleClass('auto-expand', options.as === 'textarea' && options.autoExpand !== false)
        .attr('name', options.name || name)
        .prop('disabled', options.disabled)
        .prop('readonly', options.readonly)
        .val(this.initialValueFor(name, options));
    },
    initialValueFor: function(name, options) {
      return options.value || this.object[name] || this.object[name.replace(/_(.)/g, function(a, b) {return b.toUpperCase()})];
    },
    humanizedString: function(string) {
      return string.replace(/_/g, ' ')
        .replace(/([a-z])([A-Z])/g, function(a, b, c) {return b + " " + c.toLowerCase()})
        .replace(/^./, function(a) {return a.toUpperCase()});
    },
    values: function() {
      return this.form.serializeObject();
    }
  });
});
$(document).ready(function() {
  if ($('.multi-answer-survey').length === 0) {
    return;
  }
  
  var $hispanicOrLatinoEthnicities = $('#hispanic-or-latino').find('input[type=checkbox]');
  var $otherEthnicities = $("#ethnicity").find('input[type=checkbox]');
  var $notHispanicOrLatinoOrDeclineToState = $(
    '#survey_answer_hispanic_or_latino_decline_to_state_hispanic, ' +
    '#survey_answer_hispanic_or_latino_not_hispanic_or_latino'
  );
  var $declineToStateOther = $('#survey_answer_ethnicity_decline_to_state_other');
  var $yesIAmHispanicOrLatino = $('#survey_answer_hispanic_or_latino_o');
  
  
  $hispanicOrLatinoEthnicities.click(function () {
    $yesIAmHispanicOrLatino.prop('checked', $hispanicOrLatinoEthnicities.filter(':checked').length > 0)
  });

  $notHispanicOrLatinoOrDeclineToState.click(function () {
    if ($(this).is(':checked')) {
      $hispanicOrLatinoEthnicities.prop('checked', false);      
    }
  });

  $declineToStateOther.click(function () {
    if ($(this).is(':checked')) {
      $otherEthnicities.prop('checked', false);
    }
  });
  
  $otherEthnicities.click(function () {
    if ($otherEthnicities.filter(':checked').length > 0) {
      $declineToStateOther.prop('checked', false)
    }
  });
});
$(document).ready( function() {

  $('textarea[maxlength]').keyup(function() {
    var t = $(this);
    var text = t.val();
    var limit = t.attr('maxlength');
    if (text.length > limit) {
        text = text.substring(0, limit);
        t.val(text);
    }
  });

});
$(function() {
  TourGuide = {
    begin: function(name) {
      $.get('/tours/' + name).then(function(json) {
        analytics.event('tour', 'start', name);
        TourGuide.beginSteps(json.steps, name, json);
      });
    },
    beginSteps: function(steps, name, meta) {
      var tourGuide = introJs();
      var analyticsName = name.replace(/[^a-z0-9]+/g, '-');
      meta = meta || {};

      tourGuide.setOptions($.extend({
        tooltipPosition: 'auto',
        overlayOpacity: 0.6,
        exitOnOverlayClick: false,
        steps: steps.filter(function(obj) {
          if (!obj.element) { return true; } // allow floating tooltips
          return $(obj.element).filter(':visible').length; // ignore hidden elements
        })
      }, meta.options));
      
      TourGuide.ticket(name);

      tourGuide.onchange(function(el) {
        var step = steps[this._currentStep];
        var id = step.analytics_id || step.element.replace(/[^a-z]+/g, '-').replace(/^-|-$/g, '');
        analytics.event('tour', 'step-' + id, analyticsName);
      });
      tourGuide.oncomplete(function(el) {
        analytics.event('tour', 'complete', analyticsName);
      });
      tourGuide.onexit(function(el) {
        analytics.event('tour', 'exit', analyticsName);
      });
      tourGuide.start();
    },
    ticket: function(name) {
      $.ajax({method: 'PUT', url: '/tours/' + name});
    },
    ripUpTickets: function() {
      $.ajax({method: 'DELETE', url: '/tours'});
    }
  }

  $(document).on('click', '[data-guide-name]', function(event) {
    if (event.altKey) {
      Components.modal.confirm({
        id: 'ok',
        title: 'Forget tour history?',
        body: "Forgetting your tour history will make all tours show again. Do you really want me to forget all the tours you've taken?",
        confirm: 'Yes, forget history'
      }).then(function() {
        TourGuide.ripUpTickets();
      });
    } else {
      TourGuide.begin($(this).data("guide-name"));
    }
  });
  
  var tourName = $.url().param('tour');
  tourName && TourGuide.begin(tourName);
  
  $('[data-guide-name][data-guide-version][data-guide-version-taken]').first().each(function() {
    var name = $(this).attr('data-guide-name');
    var currentVersion = Number($(this).attr('data-guide-version'));
    var takenVersion = Number($(this).attr('data-guide-version-taken'));
    if (takenVersion < currentVersion) {
      $(this).click();
    }
  });
});



























