    if (action != 'see-approvers-list') return;
    
    $('input[name=approvers_is_expanded]').val(value == 'shown');
  });

  // event listeners for approval changes to update read approval status
  $(document).on('ajax:success', '#approvers-list form#new_approver[data-remote], form.simple-form.approve[data-remote], #approvers-list a.remove-step[data-remote], #approvers-list a.remove[data-remote], form#mark-approval-unread[data-remote]', function(event, json) {
    $('.read-status').html(json['read_approval_message']);
