    group.engaged(newValue);
  });
  
  tyrantEls.find(".filter-group .option.continuum .controls").each(function() {
    var controls = $(this);
    var items = controls.find('.items li');
    var itemCount = items.length;
    var topField = controls.find(':input.anchor-top');
    var bottomField = controls.find(':input.anchor-bottom');
    var itemAtIndex = function(ix) { return items.eq(itemCount - ix) };
    var markItemAtIndex = function(ix, as) { return itemAtIndex(ix).addClass('anchor').addClass(as) }
    
    controls.find('.slider').slider({
      orientation: "vertical",
      range: true,
      animate: true,
      min: 1,
      max: itemCount,
      values: [itemCount - items.filter('.anchor-top').index(), itemCount - items.filter('.anchor-bottom').index()],
      slide: function(event, ui) {
        items.removeClass('anchor anchor-top anchor-bottom outside-anchors');
        markItemAtIndex(ui.values[0], 'anchor-top').nextAll().addClass('outside-anchors');
        markItemAtIndex(ui.values[1], 'anchor-bottom').prevAll().addClass('outside-anchors');
      },
      change: function(event, ui) {
        bottomField.val(itemAtIndex(ui.values[1]).attr('data-value'));
        topField.val(itemAtIndex(ui.values[0]).attr('data-value'));
      }
