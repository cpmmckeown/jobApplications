recruitmentEditor.controller('ApplicationFileTypesController', ['$scope', '$resource', function($scope, $resource) {
  DocumentRequirements = $resource('/analyst/recruitments/:id/document_requirements.json', {id: '@id'}, {
    update: {method: 'PUT'}
  });
  
  $scope.trueValue = true;
  $scope.falseValue = false;
  
  $('#document-requirements .loader, #document-requirements .documents.editor, #document-requirements .add-link').toggle();
  
  $('#document-requirements a.cancel').click(function () {
    ModalEditor.clearErrors($('#document-requirements'));
  });
  
  $scope.seenByApplicants = function(aft) {
    return $scope.fileOptionsLocked && aft.id;
  };
  
  $scope.willBeTardy = function(aft) {
    return $scope.fileOptionsLocked && !aft.id;
  };
  
  $scope.tardyEmailMessage = function(aft) {
    return aft.tardy_email_sent ? ', at which time an email was sent' : '';
  };

  var nextUniqueId = 1;
  $scope.uniqueId = function(aft) {
    // we need something to distinguish unsaved files for `ShownTo` ids
    return aft.id || aft.unique_id || (aft.unique_id = 'newFileNumber' + ++nextUniqueId);
  };

  $scope.save = function(event) {
    event.preventDefault();
    var modal = $('#document-requirements');
    var form = modal.closest('form');
    var reqs = new DocumentRequirements({id: $scope.recruitmentId, file_types: $scope.file_types});
    var list = reqs.$update(
      function success(reqs, headers) {
        $scope.file_types = reqs.file_types;
        ModalEditor.ajaxSuccess(form, reqs);
      },
      function error(response) {
        ModalEditor.fillErrors(form, $.extend({
          error_message: response.statusText
        }, response.data || {}));
      }
    );
  };
  
  $scope.setRequired = function(event, aft, val) {
    event.preventDefault();
    aft.is_required = val;
  };
  
  $scope.add = function(event) {
    event.preventDefault();
    var tardy = $scope.fileOptionsLocked;
    $scope.file_types.push({name: '', is_required: false, is_tardy: tardy, notify_applicants: tardy});
    $scope.reposition();
    setTimeout(function () {$('.documents.editor :text').last().focus()})
  };
  
  $scope.remove = function(event, aft) {
    event.preventDefault();
    aft.deleted = true;
    $scope.reposition();
  };
  
  $scope.restore = function(event, aft) {
    event.preventDefault();
    aft.deleted = false;
    $scope.reposition();
  };

  $scope.reposition = function() {
    // we have to do this because the old editor allowed you to start at any number and allowed skips, so we can't touch the exisitng files (it would cause an error) and we can't assume we start at 1.
    var list_order = 0;
    angular.forEach($scope.file_types || [], function (aft) {
      if ($scope.seenByApplicants(aft)) {
        list_order = aft.list_order;
      }
    });
    
    angular.forEach($scope.file_types || [], function (aft) {
      if ($scope.seenByApplicants(aft)) { return }
      
      aft.list_order = aft.deleted ? 0 : ++list_order;
    });
  };

  $scope.sortableOptions = {
    axis: 'y',
    handle: '.grip',
    items: '.new',
    opacity: .9,
    stop: function(e, ui) {
      $scope.reposition()
    }
  };
}]);
