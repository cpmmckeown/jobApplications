Appointments = function(holder) {
  this.$holder = $(holder);
};
Appointments.closest = function(el) {
  return new Appointments($(el).closest('.js-appointments'));
}
$.extend(Appointments.prototype, {
  find: function(sel) {
    return this.$holder.find(sel);
  },
  trigger: function(name) {
    this.$holder.trigger(name);
  },
  appointments: function(caveat) {
    return this.find('.appointment' + (caveat ? caveat : ''));
  },
  reindexAttributes: function(within, attrName, pattern, sub) {
    $(within).find('[' + attrName + ']').each(function(ix, el) {
      var oldValue = $(el).attr(attrName);
      var newValue = oldValue.replace(pattern, sub);
      $(el).attr(attrName, newValue);
    });
    return this;
  },
  reindex: function() {
    var appts = this;
    this.appointments().each(function(ix) {
      $(this).find('legend .number').text(ix + 1);
      
      appts.reindexAttributes(this, 'name',
        /appointments_attributes\]\[\d+\]/,
        "appointments_attributes][" + ix + "]");
      appts.reindexAttributes(this, 'id',
        /appointments_attributes_\d+_/,
        "appointments_attributes_" + ix + "_");
      appts.reindexAttributes(this, 'for',
        /appointments_attributes_\d+_/,
        "appointments_attributes_" + ix + "_");
    });
    return this;
  },
  add: function() {
    var template = this.appointments();
    var newApptFields;
  
    if (template.prop('disabled')) {
      newApptFields = template.first();
    } else {
      newApptFields = $(template[0].outerHTML);
    }
    
    // clear it out
    newApptFields.removeUniqueId();
    var newApptInputs = newApptFields.find(':input');
    newApptInputs.prop('disabled', false);
    newApptInputs.not(':checkbox, :radio').val('');
    newApptInputs.filter(':checkbox, :radio').prop('checked', false)
      .closest('label').removeClass('active');
    newApptInputs.select('[data-toggle=doomed]')
      .removeAttr('data-telescope-target')
      .removeAttr('data-doomed-is-setup');
    
    // make sure it'll be sent to the server
    this.enable(newApptFields);
    
    // make select2 happy
    newApptFields.find('.just-a-picker').show();
    newApptFields.find('.select2-container').remove();
    
    // and the datepicker, too
    var datepickers = newApptFields.find('.hasDatepicker').removeClass('hasDatepicker');
    datepickers.next().remove();
    datepickers.unwrap();
    newApptFields.appendTo(this.find('.appointments'));
    
    // and other setup ui elements
    newApptFields.find('.setup-ui').removeClass('setup-ui');

    // and integrate it in
    this.reindex();
    $(newApptFields).trigger('setup.ui');
    this.trigger('added.appointments');
    return this;
  },
  enable: function(appt) {
    return this.disable(appt, false);
  },
  disable: function(appt, disable) {
    toDisable = !!(arguments.length == 1 ? true : arguments[1]);
    
    appt.prop('disabled', toDisable);
    appt.find(':input').prop('disabled', toDisable);
    //appt.find('.select2-container').select2(toDisable ? 'disable' : 'enable');
    this.trigger((toDisable ? 'disabled' : 'enabled') + '.appointments', appt);
    return this;
  },
  setEditable: function(editable) {
    this.find('.when-editable').toggle(editable);
    this.find('.when-not-editable').toggle(!editable);

    this[editable ? 'enableAll' : 'disableAll']();
    return this;
  },
  enableAll: function() { return this.enable(this.appointments()); },
  disableAll: function() { return this.disable(this.appointments()); },
  ensureAtLeastOneAppointment: function() {
    if (this.appointments(':not(.doomed)').length < 0) this.add();
    this.enable(this.appointments(':not(.doomed)'));
    return this;
  }
});

OfferEditor = function(el) {
  this.$el = $(el);
};
OfferEditor.closest = function(el) {
  return new OfferEditor($(el).closest('.edit-offer'));
};
$.extend(OfferEditor.prototype, {
  find: function(sel) {
    return this.$el.find(sel);
  },
  visible: function(v) {
    this.$el[v ? 'slideDown' : 'slideUp']();
    return this;
  },
  offerIs: function() {
    if (arguments.length == 0) {
      return this.$el.data('offer-is');
    }
    
    var v = arguments[0];
    this.$el.data('offer-is', v);
    this.find('.offer-kind').hide();
    this.find('.when-' + v).show();
    this["when" + v[0].toUpperCase() + v.substring(1)]();
    this.updateSubmitButton();
    return this;
  },
  appointments: function() {
    return this.$appts = this.$appts || new Appointments(this.$el.is('.js-appointments') ? this.$el : this.find('.js-appointments'));
  },
  updateSubmitButton: function() {
    var appointments = this.appointments().appointments(':not(.doomed)');
    var offerIsInvalid = this.offerIs() == 'required' && appointments.length == 0;
  
    this.$el.closest("form").find(':submit').prop('disabled', offerIsInvalid);
  },
  whenForbidden: function() {
    this.appointments().setEditable(false);
    this.visible(false);
  },
  whenOptional: function() {
    var keepIfVisible = $(".when-editable").is(":visible");
    this.appointments().setEditable(keepIfVisible);
    this.visible(true);
  },
  whenRequired: function() {
    this.appointments().setEditable(true);
    this.visible(true);
  },
});

