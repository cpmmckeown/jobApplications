        $this.select2('open');
      }, 0);
    });
  }
  
  function formatGroupAndItem(fn) {
    return function(object) {
      var element = object.element && object.element[0];
      var optGroup = element && element.parentElement.getAttribute('label');
      return fn(optGroup, object.text, optGroup, object);
    }
  }
  
