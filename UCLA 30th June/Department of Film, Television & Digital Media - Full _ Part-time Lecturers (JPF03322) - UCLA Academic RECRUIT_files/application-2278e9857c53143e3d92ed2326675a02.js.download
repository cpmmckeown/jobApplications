      var alert = modal.find('.alert');
      
      modal.find('button').prop('disabled', !can);
      if (!can) {
        alert.find('.message').empty().append(why);
      }
      alert.toggleClass('starts-hidden', can);
    }
  }
  
  // approval creation
  var previewApprovalFor = function (form, fn) {
    $.ajax({
      url: form.attr('action') + '/new.json',
      data: form.serializeObject(),
      success: fn
    });
  };
  $(document).on('click', '.new-approval-link', function(event) {
    var $this = $(this)
    var modal = $($(this).attr('data-target'));
    var form = modal.closest('form.approval');
    var rawAttrs = $(this).attr('data-approval');
    var attrs = JSON.parse(rawAttrs);
    
    form.find('.autoadd').remove();
    $.each(attrs, function(key, value) {
      Approval.findOrCreateHiddenFieldIn(form, key).val(value);
    });
    
    var preview = modal.find('.approval-preview');
    if (preview.length > 0) {
      previewApprovalFor(form, function (data) {
        preview.empty();
        if (!data.possible_workflows) { return; }
        var chosenWorkflow = data.possible_workflows[0];
        if (!chosenWorkflow || !chosenWorkflow.id) { return; }
        
        var using = $('<p class="workflow">Using workflow "<span class="name"></span>", which includes the following steps:</p>');
        using.find('.name').text(chosenWorkflow.name);
        using.appendTo(preview);
        
