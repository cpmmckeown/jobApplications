      $.extend(opts, {width: '100%'});
    }

    $this.data('original-select2-opts', opts);
    $this.select2(opts);
    $this.prop('disabled', $this.closest('fieldset[disabled], .form-group.disabled').length > 0);
    
    if ($this.is('.autosubmit')) {
      $this.change(function() {this.form.submit()});
    }

    var model = $this.attr('ng-model');
    if (model && $this.scope() && $this.scope().$watch) {
