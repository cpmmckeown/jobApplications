function toggleMessage(toggleController) {
  var targetId = toggleController.data('toggle-target') || toggleController.attr('id').replace(/toggle_for_/, '');
  var target = $("#"+targetId);
  var showMessage = toggleController.data('show-message') || "Show";
  var hideMessage = toggleController.data('hide-message') || "Hide";
  
  if (target.is('.hidden')) target.hide().removeClass('hidden');
  
  target.toggle('slow');
  if ($(toggleController).html() == showMessage) {
    $(toggleController).html(hideMessage);
  } else {
    $(toggleController).html(showMessage);
  }
}

function dialogMessage(dialogController) {
  var targetId = dialogController.attr('id').replace(/dialog_for_/, '');
  $("#"+targetId).dialog();
}

