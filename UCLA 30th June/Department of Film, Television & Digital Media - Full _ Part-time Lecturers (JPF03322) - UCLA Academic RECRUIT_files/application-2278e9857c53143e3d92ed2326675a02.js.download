$(function() {
  // trap focus in active modals
  $(document).on('focusin', function(event) {
    var $modal = $('.modal.in');
    if ($modal.length && $(event.target).closest('.modal.in, .select2-container').length === 0) {
      $modal.find(':focusable').first().focus();
    }
  });

  $(document).on('ajax:success ajax:error', function() {
    addAriaToFormFields() 
  });

  addAriaToFormFields();

  function addAriaToFormFields() {
    $('.controls .required').attr('aria-required', 'true');
    $('.help-block.error').each(function() {

      // help-block class not alwyas on the error sometimes on the parent element
      // this happens to be true for form fields that are not in the GED
      if ($(this).parent().hasClass('help-block')) {
        // not GED
        var helpBlock = $(this).parent();
      } else {
        // GED
        helpBlock = $(this);
      }

      var field = helpBlock.prev();

      var field_id = field.attr('id');

      if (field.hasClass('select2')) { // handling select2
        // so that placeholders won't be read twice
        field.find('.select2-selection__placeholder').attr('aria-hidden', 'true');

        // find actual select2 target
        field = field.find(".select2-selection");

        targetField = field.parent().parent().prev()
        if (targetField.attr('aria-required') === "true") {
          field_id = targetField.attr('id');
        } else {
          console.error('Unable to find correct select2 target field');
        }
      } else if (field.hasClass('input-group')) {
        // for handling date fields
        field = field.children().first();
      }

      field.attr('aria-invalid', 'true');
      helpBlock.attr('id', field_id + '-error-description');
      field.attr('aria-describedby', helpBlock.attr('id'));
    });
  }
});
