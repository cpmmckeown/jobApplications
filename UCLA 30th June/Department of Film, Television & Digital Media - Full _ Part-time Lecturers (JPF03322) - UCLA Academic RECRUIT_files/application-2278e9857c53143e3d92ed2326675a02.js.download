  tyrantEls.on('click', '.edit-workspace .save', function(e) {
    TableTyrant.Grid.closest(e.target).event('filters-set');
  });
  
  // dynamic filtering
  tyrantEls.find('.filter-includes li').each(function() {
    var $gridEl = $(this).closest('.table-tyrant');
    var grid = new TableTyrant.Grid($gridEl);
    var include = $($(this).text())
    var dynamicFilters = include.find("input.dynamic-filter");
    var showingRows = function() {
      return $gridEl.find('.matches tr.match:not(.filtered-out)');
    }

    include.off();
    include.submit(function(e) {
      e.preventDefault();

      if (searchOnServer) {
        grid.event('search');
        grid.updateWorkspace();
      }
    });

    var tableRows = $gridEl.find('.matches tr.match');

    $.extend($.expr[':'], {
      containsSearchData: function(a,i,m){
      return ($(a).attr('data-search') || '').indexOf(m[3]) >= 0;
      }
    });
    $.fn.dynamicSearch = function(q) {
      this.filter(':containsSearchData("'+q+'")').removeClass('hidden');
      this.filter(':not(:containsSearchData("'+q+'"))').addClass('hidden');
    };

    dynamicFilters.bind('keyup click change', function(e) {
      if ($(".more-pages-to-see").length) return;
      var input = $(e.target);
      var searchTerm = input.val().toLowerCase();

      //otherwise we have all the records we need to filter on our own
      // hide rows that don't match
      var isMatch = tableRows.filter(':containsSearchData("'+searchTerm+'")');
      var notMatch = tableRows.filter(':not(:containsSearchData("'+searchTerm+'"))');

      isMatch.removeClass('hidden');
      notMatch.addClass('hidden');

      tableRows.removeClass('filtered-in filtered-out');
      isMatch.addClass('filtered-in');
      notMatch.addClass('filtered-out');

      // hide empty sections, if enabled
      if ($gridEl.attr('data-search-hides-empty-sections') === "true") {
        $gridEl.find('tbody.matches').show(); //or we can't tell what hidden sections now contain matches
        
        // hide groups that have no matches
        $gridEl.find('tbody.matches tr.group-name').each(function(ix, el) {
          var hasMatches = $(el).nextUntil('.group-name').is(':visible');
          var hasSearch = searchTerm.length > 0;
          var shouldShow = (hasSearch && hasMatches) || !hasSearch;

          $(el).toggle(shouldShow);
        });
        // hide sections that have no matches
        $gridEl.find('tbody.matches').each(function(ix, el) {
          var hasMatches = $(el).find('tr.match.filtered-in').length > 0;
          var hasSearch = searchTerm.length > 0;
          var shouldShow = (hasSearch && hasMatches) || !hasSearch;
          
          $(el).prev('thead').toggle(shouldShow);
          $(el).toggle(shouldShow);
          $(el).next('tbody.summary').toggle(shouldShow);
        });
        
        $gridEl.find('tbody.no-matching-sections').toggle($gridEl.find('tbody.matches .filtered-in').length === 0);
      }
      
      // recolor the alternating rows
      $gridEl.find('.matches tr').removeClass('filter-odd').removeClass('odd');
      $gridEl.find('.matches tr.filtered-in').filter(function(ix) {return ix % 2 === 1}).addClass('filter-odd');
       
      // update the counts or show warnings
      $gridEl.find('tbody.matches').each(function(ix, el) {
        var matches = $(el);
        var summary = matches.next('tbody.summary')
        var showing = matches.find('tr.match.filtered-in').length;
        var hasSearch = searchTerm.length > 0;
        var noResults = matches.find('.filter-no-results');

        noResults.toggleClass('hidden', !hasSearch || showing > 0);
        summary.find('.showing').toggleClass('hidden', !hasSearch);

        if (hasSearch) {
          summary.find('.showing .n').text(showing);
          summary.find('.showing .item-name').text(showing === 1 ? 'match' : 'matches');
        }
      });
    }).keyup();

    $gridEl.find('.edit-workspace').submit(function(e) {
      include.each(function() {
        $.each($(this).serializeArray(), function(ix, obj) {
          $('<input/>').attr({name: obj.name, value: obj.value, type: 'hidden'}).appendTo(e.target);
        });
      });
    });
  });

