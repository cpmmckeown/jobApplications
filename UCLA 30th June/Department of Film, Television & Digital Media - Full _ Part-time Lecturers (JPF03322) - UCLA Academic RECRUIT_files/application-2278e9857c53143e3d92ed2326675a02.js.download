  tyrantEls.on('keydown', '.column-selector .column', function(event) {
    var grid = TableTyrant.Grid.closest(event.target);
    
    switch (event.which) {
      case 13: // enter
        grid.columns().toggleVisible(event.target);
        break;
      case 37: // left
        grid.columns().makeVisible(event.target);
        break;
      case 39: // right
        grid.columns().makeHidden(event.target);
        break;
      case 38: // up
        grid.columns().moveUp(event.target);
        break;
      case 40: // down
        grid.columns().moveDown(event.target);
        break;
    }
  });
  tyrantEls.on('click', '.column-group', function(event) {
