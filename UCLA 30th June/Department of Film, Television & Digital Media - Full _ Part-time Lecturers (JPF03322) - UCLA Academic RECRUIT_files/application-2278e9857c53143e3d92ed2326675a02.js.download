          style.verticalAlign = style.verticalAlign || 'middle';
          var tipImage = trigger = $("<img class='tooltip-popup-trigger' src='" + tipImageUrl + "'/>");
          tipImage.attr('alt', tipDiv.text());
          tipImage.css(style);
          
          if ($(this).is('th')) {
            $(this).append(tipImage);
          } else {
            $(this).after(tipImage);
          }
        }
        
        // and show it when they hover over the help logo
        var showTooltip = function(e) {
            var winWidth = $(window).width();
            var triggerPos = trigger.offset();
            var triggerWidth = trigger.width();
            var leftMargin = triggerPos.left + triggerWidth;
            var rightMargin = winWidth - triggerPos.left;
            
            tipDiv.appendTo($('body')).
                css({
                    position: "absolute",
                    top: (triggerPos.top) + "px"
                });
            
            var roomOnRight = winWidth > leftMargin + tipDiv.outerWidth();
            if (options.alwaysOn == "bottom") {
                tipDiv.css("top", (triggerPos.top + trigger.outerHeight()) + "px");
                tipDiv.css("left", triggerPos.left + "px");
            } else if (roomOnRight || options.alwaysOn == "right") {
                tipDiv.css("left", leftMargin + "px");
            } else {
                tipDiv.css("right", rightMargin + "px");
            }
            
            tipDiv.show();
        };
        var hideTooltip = function() {
            tipDiv.hide();
        };

