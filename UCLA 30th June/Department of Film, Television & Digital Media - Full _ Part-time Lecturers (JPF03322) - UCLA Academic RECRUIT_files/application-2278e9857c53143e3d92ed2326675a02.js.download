/* Used for interdependant show/hide behavior.

The basic mechanic is this:

  <div class='shown-to-see-more starts-hidden'>Blah blah Blah blah</div>
  <span data-shown-to='see-more'>Show</span>

The <div> starts hidden (as per the class), but when uou click the <span> it shows up.

You can also do the opposite:

  <div class='hide-to-see-more'>Blah blah Blah blah</div>
  <span data-shown-to='see-more'>Hide</span>

Which will hide the <div> when you click the <span>.

You can also have multiple actions all occur at once:

  <div class='hide-to-see-more'>Blah blah...</div>
  <div class='shown-to-see-more starts-hidden'>Blah blah Blah blah</div>
  <span data-shown-to='see-more' class='hide-to-see-more'>More</span>

That will show the first <div> and the <span> to start,
but when you click the <span> it will show only the second <div>.

You can also have multiple triggers, if you want: just share the same `data-shown-to`.
We keep track of shown/hidden state on the page, so all triggers stay in sync

Finally, you might want to check the state to do your own thing with it:

  ShownTo.when('see-more', true, function() {alert('I can see more!')});

*/


ShownTo = {
  get: function(action) {
    action = action.data ? action.data('shown-to') : action;
    return !!$(document.body).data(action);
  },
  set: function(action, value) {
    action = action.data ? action.data('shown-to') : action;
    return $(document.body).data(action, value).trigger('change.shown-to', [action, (value ? 'shown' : 'hidden')]);
  },
  when: function(action, is, fn) {
    if (ShownTo.get(action) == is) fn();
  },
  show: function(action) {
    ShownTo.when(action, false, function() {ShownTo.toggle(action)});
  },
  hide: function(action) {
    ShownTo.when(action, true, function() {ShownTo.toggle(action)});
  },
  enableBy: function(el) {
    ShownTo.when(el, false, function() {el.click()});
  },
  disableBy: function(el) {
    ShownTo.when(el, true, function() {el.click()});
  },
  toggle: function(el) {
    if (typeof(el) == "string") el = $('*[data-shown-to="' + el + '"]');
    
    var action = el.attr('data-shown-to');
    var howToToggle = el.attr('data-shown-to-by') || 'slideToggle';
    var focusOn = el.attr('data-shown-to-focus-target');
    
    var was = ShownTo.get(action);
    var is = !was;
    ShownTo.set(action, is);
    
    $('.hidden-to-' + action + ', .shown-to-' + action)[howToToggle]();
    $('.toggled-to-' + action).toggle();
    $('.inline-to-' + action).css({display: is ? 'inline' : 'none'});
    $('.enabled-to-' + action).prop('disabled', !is);
    $('.enabled-to-' + action + '.hasDatepicker').datepicker('option', 'disabled', !is);
    $('.disabled-to-' + action).prop('disabled', is);
    if (focusOn) {
      focusOn = $(focusOn)
      if (focusOn.is('.select2-offscreen')) {
        focusOn.select2('open');
      } else if ($(focusOn).is(':visible')) {
        $(focusOn).focus();
      }
    }
    return !el.is('a[href="#"], button, input[type=submit], input[type=button]');
  },
  radioGroup: {
    toggleTo: function(name, newValue, el) {
      var inGroup = $('[data-shown-to-radio-group="' + name + '"]');
      var triggers = inGroup.filter('[data-shown-to-radio]');
      var targets = inGroup.not(triggers);
      var allVals = $.map(triggers, function(t) {return $(t).attr('data-shown-to-radio')});
      var otherVals = $.map(allVals, function(v) {if (v != newValue) return v});
      var hiddenTos = $.map(otherVals, function(v) {return '.hidden-to-' + v}).join(', ') || '.never-matches-anything';
      var shownTos = $.map(otherVals, function(v) {return '.shown-to-' + v}).join(', ') || '.never-matches-anything';
      var howToToggle = ($(el).attr('data-shown-to-by') || 'slideUp|slideDown').split('|');
      
      $(['.shown-to-' + newValue, hiddenTos].join(', '))[howToToggle[1]](false);
      $(['.hidden-to-' + newValue, shownTos].join(', '))[howToToggle[0]](false);
      
      return $(el || document.body).trigger('change.shown-to', [name, newValue]);
    }
  }
};

