      var activeLink = ActiveTab.linksFor(tabKey).closest('li.active').find('a');
      history.replaceState({newTab: ActiveTab.valueForLink(activeLink), tabKey: tabKey}, null, activeLink.data('url') + fragment);
    }
  };
  
