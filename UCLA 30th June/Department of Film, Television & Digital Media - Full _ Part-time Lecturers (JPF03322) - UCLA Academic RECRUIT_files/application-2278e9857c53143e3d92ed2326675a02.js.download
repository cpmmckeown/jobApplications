$(function() {
  Appointment = {
    updateAll: function() {
      Appointment.forEach($('fieldset.appointments'), function($appt) {
        Appointment.ComputedSalary.updateAll($appt);
        Appointment.Salary.hideShow($appt);
      });
    },
    forEach: function(within, fn) {
      $(within || document).find('.appointment').each(function() {
        fn($(this));
      });
    },
    
    ComputedSalary: {
      update: function($appt) {
        var percent = Number($appt.find('.percent-time input').val()) / 100;
        var fteSalary = $appt.find('.salary input').val();
        fteSalary = parseFloat(fteSalary && fteSalary.replace ? fteSalary.replace(/,/g, '') : fteSalary).toFixed(2);
        var salaryField = $appt.find('.computed-salary input');
        var computedSalary = fteSalary * percent;
        if (computedSalary === 0 || isNaN(computedSalary)) {
          analytics.event('appointment-computed-salary', 'hide', $appt.attr('id'));
          salaryField.closest('.form-group').hide();
        } else {
          analytics.event('appointment-computed-salary', 'show', $appt.attr('id'), computedSalary);
          salaryField.closest('.form-group').show();
          salaryField.val(computedSalary);
        }
      },
      updateAll: function(within) {
        Appointment.forEach(within, Appointment.ComputedSalary.update);
      }
    },
    
    Salary: {
      toggle: function($appt, show) {
        var action = show ? 'show' : 'hide';
        analytics.event('appointment-salary', action, $appt.attr('id'));
        var input = $appt.find('.salary.form-group')[action]().find(':input');
        if (!show) {input.val('')}
        input.trigger('change');
      },
      hideShow: function($appt) {
        var salaryPresent = !!$appt.find('.salary.form-group :input').val();
        var hasSalary = $appt.find('.has_salary :input:checked').val() || String(salaryPresent);
        Appointment.Salary.toggle($appt, hasSalary === "true");
      },
      hideShowAll: function(within) {
        Appointment.forEach(within, Appointment.Salary.hideShow);
      }
    }
  };
  
  $(document).on('keyup change', [
    'fieldset.appointments .appointment .percent-time input',
    'fieldset.appointments .appointment .salary :input'
  ].join(", "), function(event) {
    Appointment.ComputedSalary.update($(event.target).closest('.appointment'));
  });

  $(document).on('change', [
    '.appointment select.title-picker',
    '.appointment .has_salary :radio'
  ].join(", "), function(event) {
    Appointment.Salary.hideShow($(event.target).closest('.appointment'));
  });
  
  Appointment.updateAll();
  $(document).on('added.appointments enabled.appointments setup.ui', function(event) {
    Appointment.updateAll();
  });
});
