dispositionReasonsManager.controller('ReasonController', ['$scope', function($scope) {
  $scope.isEditing = delete $scope.reason.isEditing;

  $scope.shouldBeHidden = function() {
    var reason = $scope.reason;
    var editedName = reason.editedName || '';
    return !reason.id && reason.trashed && editedName == '';
  };

  $scope.toggleEditing = function(event) {
    event.preventDefault();
    var was = $scope.isEditing;
    $scope.resetIsEditing();
    $scope.isEditing = !was;
  };

  $scope.resetInputError = function() {
    var reason = $scope.reason;
    if (reason.editedName || reason.trashed) {
      delete reason.inputError;
    }
  };

  $scope.$watch('isEditing', function(newValue) {
    if (newValue == true) {
      setTimeout(function () {
        $scope.editor().find('li.reason input:visible').focus();
      });
    } else {
      var reason = $scope.reason;
      reason.editedName = reason.editedName || '';
      if (reason.editedName == '') {
        if (reason.id) {
          $scope.cancelEditing();
        } else {
          reason.trashed = true;
        }
      }
      $scope.resetInputError();
    }
  });

  $scope.cancelEditing = function(event) {
    var reason = $scope.reason;
    reason.editedName = reason.lastEditedName || reason.name || ''; 

    event && $scope.toggleEditing(event);
  };

  $scope.saveEditing = function(event) {
    if ($scope.reason.editedName) {
      $scope.reason.lastEditedName = $scope.reason.editedName;
      $scope.toggleEditing(event);
    } else {
      $scope.reason.inputError = true;
    }
  };

  $scope.toggleTrash = function(event) {
    event.preventDefault();
    var was = $scope.reason.trashed;
    if (was) {
      $scope.reason.trashed = !was;
    } else if (confirm("Really delete the reason \"" + $scope.reason.editedName + "\"?")) {
      $scope.reason.trashed = !was;
    }
  };

  $scope.$on("resetIsEditing", function() {$scope.isEditing = false});
}]);
