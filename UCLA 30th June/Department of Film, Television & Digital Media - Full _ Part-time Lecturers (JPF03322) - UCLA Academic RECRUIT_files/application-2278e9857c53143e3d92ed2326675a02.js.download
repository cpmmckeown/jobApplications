DuplicateID = {
  config: function(key, value) {
    var defaults = {
      action: "ignore",
      logEachDuplicate: true,
      logTotal: true
    };
    var cookieName = 'duplicateId.' + key;
    return value ? $.cookie(cookieName, value) : $.cookie(cookieName) || defaults[key];
  },
  updateAction: function(event) {
    event.preventDefault();
    DuplicateID.config('action', $(event.target).data('duplicate-id-action'));
    DuplicateID._highlightCurrentAction();
  },
  scan: function() {
    var seen = [];
    return $('*[id]').map(function() {
      if (seen.indexOf(this.id) > -1) {
        return this.id;
      }
      seen.push(this.id);
    }).toArray().filter(function(id, ix, self) {
      return self.indexOf(id) > -1 && self.indexOf(id) === ix;
    }).sort();
  },
  complain: function() {
    if (DuplicateID.config('action') === 'ignore') return;
    if ($('html.classic').length > 0) return;
    
    var duplicates = DuplicateID.scan();
    if (duplicates.length) {
      DuplicateID._showModal(duplicates);
      DuplicateID._addNavigationLink();
    }
  },
  highlightAll: function(duplicates) {
    (duplicates || DuplicateID.scan()).forEach(function(dupe) {
      DuplicateID.highlightId(dupe);
    });
  },
  highlightId: function(id) {
    DuplicateID._elementsWithId(id).each(function() {
      var highlight = $(this);
      while(highlight.css('display') !== 'block') highlight = highlight.parent();
      highlight.css({border: "10px solid " + (highlight[0] === this ? 'red' : 'pink')});
    });
  },
  
  // private methods
  _elementsWithId: function(id) {
    return $('*[id="'+id+'"]');
  },
  _summaryForId: function(id) {
    var dupes = this;
    console.group("Duplicates with ID " + id);
    
    var codeBlocks = $('<ul>');
    DuplicateID._elementsWithId(id).each(function() {
      dupes.count++;
      if (DuplicateID.config('logEachDuplicate')) {console.dir(this);}
      
      $("<li>")
        .append($("<span class='metadata'>").append(Sherlock.pathTo(this)))
        .append($("<br/>"))
        .append($("<code>").append(Sherlock.html(this)))
        .appendTo(codeBlocks);
    });
    
    console.groupEnd();
    return $("<div>")
      .append($("<h4>")
        .append(id)
        .append(' ')
        .append(Components.iconic('search'))
        .on('click', function() {DuplicateID.highlightId(id)}))
      .append(codeBlocks);
  },
  _highlightCurrentAction: function() {
    var action = DuplicateID.config('action');
    $('[data-duplicate-id-action]').css("font-weight", "400")
      .filter('[data-duplicate-id-action="'+action+'"]').css("font-weight", "800");
  },
  _showModal: function(duplicates) {
    var preamble = "This page has <strong>%{num}</strong> duplicate IDs. These will cause issues with JavaScript, especially if one of them is a modal, tooltip, or picker. The following IDs are duplicated:";
    var postamble = "It is likely that the IDs are auto-generated based on user content (document names, applicant names, etc) or models (`user_2`, `recruitment_name`). You should find some way to de-duplicate them.";
    var actionEditor = $('<small>')
      .css({marginLeft: '1em'})
      .append("When I find duplicates, I will ")
      .append($("<a href='#' onclick='DuplicateID.updateAction(event)' data-duplicate-id-action='popup'>popup</a>"))
      .append(" | ")
      .append($("<a href='#' onclick='DuplicateID.updateAction(event)' data-duplicate-id-action='detect'>detect but ignore</a>"))
      .append(" | ")
      .append($("<a href='#' onclick='DuplicateID.updateAction(event)' data-duplicate-id-action='ignore'>ignore entirely</a>"))
      .append(".")
    var dupes = this;
    
    var actionMethod = {
      popup: 'confirm',
      detect: 'confirmation',
      ignore: 'ignore'
    }[DuplicateID.config('action')];
    var modal = Components.modal[actionMethod]({
      id: 'duplicate-ids',
      title: function(title) {
        title.append('Duplicate IDs in DOM'); 
        title.append(actionEditor);
