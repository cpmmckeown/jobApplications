$(function() {
  window.FindAndUpdate = {
    whenModalCloses: function(fn) {
      var callbacks = $.Deferred();
      var currentModal = $(".modal.in");
      var fnAndThen = function() {
        fn();
        callbacks.resolve();
      }

      if (currentModal.length > 0) {
        currentModal.
          on('hidden.bs.modal', fnAndThen).
          modal('hide');
      } else {
        fnAndThen();
      }
      return callbacks;
    },
    
    text: function(updates) {
      $.each(updates || {}, function (selector, content) {
        $(selector).text(content);
      });
    },

    htmlImmediate: function(updates) {
      $.each(updates || {}, function (selector, content) {
        $(selector).replaceWith(content);
      });
      $(document).trigger('setup.ui');
    },

    html: function(updates) {
      return FindAndUpdate.whenModalCloses(function() {
        FindAndUpdate.htmlImmediate(updates);
      });
    },
    
    glow: function(glows) {
      glows = glows || {};
      
      $(glows.created).addClass('target-of-create');
      $(glows.updated).addClass('target-of-update');
      $(glows.destroyed).addClass('target-of-destroy');
      
      Glow.created($('.target-of-create'))
        .then(function(g) {g.el().removeClass('target-of-create')});
      Glow.updated($('.target-of-update'))
        .then(function(g) {g.el().removeClass('target-of-update')});
      Glow.destroyed($('.target-of-destroy'))
        .then(function(g) {g.el().removeClass('target-of-destroy')});
    },

    classes: function(updates) {
      $.each(updates || {}, function(selector, classes) {
        $(selector).addClass(classes.add);
        $(selector).removeClass(classes.remove);
      });
    }
  };

  $(document).on('ajax:success', '[data-find-and-update~="text"]', function(e, json, _status, _xhr) {
    FindAndUpdate.text(json.text_updates);
  });

  $(document).on('ajax:success', '[data-find-and-update~="html"]', function(e, json, _status, _xhr) {
    FindAndUpdate.html(json.updates).then(function() {
      FindAndUpdate.glow(json.glows);
    });
  });

  $(document).on('ajax:error', '[data-find-and-update~="html"]', function(e, xhr, errorname, error) {
    var json = xhr.responseJSON;
    if (json && json['updates']) {
      FindAndUpdate.html(json.updates).then(function() {
        FindAndUpdate.glow(json.glows);
      });
    } else {
      alert(json && json['error_message'] ? json['error_message'] : 'Something went wrong');
    }
  });
});
