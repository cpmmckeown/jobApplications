    if (matchConflicts) {
      var conflictKind = matchingStep.data('name') == $this.val() ? '' : 'equivalent';
      Approver.canAdd(modal, false, 'A step with the ' + conflictKind +
        ' role "' + matchingStep.data('name') +
        '" already exists in the approval.')
