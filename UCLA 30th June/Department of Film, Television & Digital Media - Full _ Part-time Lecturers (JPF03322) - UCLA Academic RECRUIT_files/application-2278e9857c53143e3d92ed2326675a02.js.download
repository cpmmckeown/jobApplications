approvalWorkflowsEditor.controller('ApprovalWorkflowController', ['$scope', '$resource', function($scope, $resource) {
  Workflow = $resource('/admin/approval_workflows/:id.json', null, {
    update: {method: 'PUT'}
  });
  
  var m = document.location.href.match(/approval_workflows\/(\d+)/);
  if (m) {
    $scope.workflow = Workflow.get({id: m[1]}, function () {
      $scope.loaded = true
    });
  } else {
    $scope.workflow = new Workflow();
    $scope.loaded = true;
  }
  
  $scope.save = function (event) {
    event.preventDefault();
    var w = $scope.workflow;
    var method = w.id ? '$update' : '$save';
    
    w[method]({id: w.id}, function (newW, headers) {
      document.location.href = '/admin/approval_workflows';
    }, function (response) {
      w.errors = response.data['errors'];
    });
  };
  
  $scope.roleName = function(roleId) {
    return $('select.approver-role-picker option[value=' + roleId + ']').text();
  };

  $scope.permission = function(roleId) {
    return $('select.approver-role-picker option[value=' + roleId + ']').data('permission-name');
  };

  $scope.forStepsWithPermission = function(permission, fn) {
    angular.forEach($scope.workflow.steps, function(s) {
      if ((s.permission_for_role == permission) && (s.deleted != true)) {
        fn(s);
      }
    });
  };

  $scope.setCondition = function(condition, value) {
    ($scope.workflow.conditions_recipe = $scope.workflow.conditions_recipe || {})[condition] = value;
  };
  
  $scope.setConditionOption = function (condition, name, value) {
    var cond = $scope.workflow.conditions_recipe[condition];
    cond[name] = value;
  };
  
  $scope.hasSteps = function () {
    var workflow = $scope.workflow;
    return workflow && $.grep(workflow.steps || [], function (step) {return !step.deleted}).length > 0;
  };
  
  $scope.validateStep = function (step) {
    $scope.workflow.steps = $scope.workflow.steps || [];
    var isValid = true;

    $scope.forStepsWithPermission(step.permission_for_role, function(s) {
      step.duplicate_step = s
      isValid = false;
    });
    return isValid;
  };

  $scope.addStep = function(step) {
    if (step.preventDefault) {
      step.preventDefault();
      $scope.$broadcast('newStep');
      return;
    }
    
    $scope.workflow.steps = $scope.workflow.steps || [];
    step.permission_for_role = $scope.permission(step.role_id);

    if ($scope.validateStep(step)) {
      $scope.workflow.steps.push(step);
      $scope.reposition();
    } else {
      $scope.$broadcast('replaceStep', step);
    }
  };

  $scope.removeStep = function(step) {
    step.deleted = true;
    $scope.reposition();
  };
  
  $scope.reposition = function() {
    var position = 1;
    angular.forEach($scope.workflow.steps || [], function (step) {
      step.position = step.deleted ? 0 : position++;
    });
  };
  
  $scope.sortableOptions = {
    axis: 'y',
    cursor: 'move',
    handle: '.grip',
    opacity: .75,
    stop: function(e, ui) {
      $scope.reposition()
    }
  };
  
  $scope.hasErrorOn = function (pattern) {
    return $.grep($scope.workflow.errors || [], function (error) {
      return error.indexOf(pattern) > -1;
    }).length > 0;
  };
}]);

