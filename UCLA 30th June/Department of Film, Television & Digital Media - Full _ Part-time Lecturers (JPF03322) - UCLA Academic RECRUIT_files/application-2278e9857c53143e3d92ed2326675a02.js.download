        }, delay || 1000)
      });
    },
    _analytics: function(action, id) {
      var glow = this;
      this.frame().each(function(ix, frame) {
        analytics.event('glow', action, id || glow._analytics_id_for($(frame)));
      });
    },
    _analytics_id_for: function($frame) {
      var id = $frame.attr('analytics-id') || $frame.attr('id');
