      if (link.data('url')) return link.data('url');
      
      var oldUrl = $.url();
      var oldFrag = oldUrl.attr('fragment');
      oldFrag = oldFrag ? ('#' + oldFrag) : '';
      if ($(oldFrag + '.modal').length) oldFrag = '';
      
      var newParams = oldUrl.param();
      newParams[tabKey] = value;
      
      return oldUrl.attr('path') + '?' + $.param(newParams) + oldFrag;
    },
    
    get: function(tabKey) {
      return (history.state && history.state.newTab) || ActiveTab.valueForLink(ActiveTab.defaultLink(tabKey));
    },
    
    set: function(tabKey, value) {
      if (history.state && history.state.newTab == value) return;
      
      var state = {newTab: value, tabKey: tabKey};
      var newUrl = ActiveTab.urlFor(tabKey, value);
      history.pushState(state, null, newUrl);
      ActiveTab.navTabs(tabKey).trigger('change.active-tab', state);
    },
    
    setUp: function(tabKey) {
