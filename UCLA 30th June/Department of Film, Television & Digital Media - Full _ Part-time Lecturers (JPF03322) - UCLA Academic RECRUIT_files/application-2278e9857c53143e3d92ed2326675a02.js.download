        if (!options.onText) {
          // insert the help logo
          var tipImageUrl = options.imageUrl || "/assets/help.png";
          var style = options.style || {};
          if ($(this).attr('data-tooltip-style')) {
            style = $.parseJSON($(this).attr('data-tooltip-style'));
