dispositionReasonsManager.controller('CustomReasonsController', ['$scope', '$sce', '$filter', function($scope, $sce, $filter ) {
  $scope.CUSTOM_LIST_LIMIT = 5;

  $scope.resetTrashed = function() {
    angular.forEach($scope.list.custom_reasons, function(reason) {
      if (reason.id) {
        delete reason.trashed;
      } else {
        reason.trashed = true;
      }
    });
  };
  $scope.resetEditedValues = function() {
    angular.forEach($scope.list.custom_reasons, function(reason) {reason.editedName = reason.name});
  };
  $scope.resetEditedValues();
  $scope.resetIsEditing = function() {
    $scope.$broadcast("resetIsEditing");
  };

  $scope.toBeSavedCount = function() {
    var toBeTrashed = $filter('filter')($scope.list.custom_reasons, {trashed: true});
    return $scope.list.custom_reasons.length - toBeTrashed.length;
  };

  $scope.editor = function() { 
    return $('#' + $scope.category + " " + ".custom-reasons-editor"); 
  };

  $scope.modal = function(event, action) {
    event.preventDefault();

    if (action == "show") {
      $scope.resetEditedValues();
      $scope.resetIsEditing();
      $scope.resetTrashed();
    } else if (action == "hide") {
      $scope.resetTrashed();
    }

    $scope.editor().modal(action);
  };

  $scope.addReason = function(event) {
    event && event.preventDefault();
    $scope.resetIsEditing();
    $scope.list.custom_reasons.push({isEditing: true});
  };

  $scope.prepareCustomReasons = function () {
    angular.forEach($scope.list.custom_reasons, function(reason) {
      if (reason.editedName) {
        reason.name = reason.editedName;
      }
      if (reason.editedName && reason.inputError) {
        delete reason.inputError;
      }
      if (!(reason.editedName || reason.trashed)) {
        reason.inputError = true;
      }
    });
  };

  $scope.inputErrors = function () {
    var withError = $filter('filter')($scope.list.custom_reasons, {inputError: true});
    var withErrorTrashed = $filter('filter')(withError, {trashed: true});
    return (withError.length - withErrorTrashed.length > 0);
  };

  $scope.saveChanges = function(event) {
    event.preventDefault();
    $scope.prepareCustomReasons();

    if (!$scope.inputErrors()) {
      $scope.configuration.$update({code: $scope.recruitmentId}, function (c, responseHeaders) {
        $scope.modal(event, "hide");
      });
    }
  };
}]);

